<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.workshop.cotic.feedback.VisualizarPalestra">

    <TextView
        android:id="@+id/visualizarTitulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="29dp"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="165dp" />

    <TextView
        android:id="@+id/visualizarTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/visualizarTitulo"
        android:layout_below="@+id/visualizarTitulo"
        android:layout_marginTop="35dp"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="165dp" />

    <Button
        android:id="@+id/visualizar_avaliar_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="66dp"
        android:text="Avaliar Palestra"
        android:layout_below="@+id/visualizarTipo"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/visualizar_delete_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/visualizarTitulo"
        android:layout_marginEnd="73dp"
        app:srcCompat="@android:drawable/ic_menu_delete" />

</RelativeLayout>
